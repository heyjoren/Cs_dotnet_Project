<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WishList.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:WishList"
    xmlns:Itemsviews="clr-namespace:WishList.View.Items"
    xmlns:AddItemsviews="clr-namespace:WishList.View.Items.Add"
    Title="WishList">

    <TabBar>
        <!--<ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />-->
        <ShellContent
            Title="Items"
            ContentTemplate="{DataTemplate Itemsviews:Items}"
            Route="Items" />
        <ShellContent
            Title="Add item"
            ContentTemplate="{DataTemplate AddItemsviews:addItem}"
            Route="AddItem" />
    </TabBar>
</Shell>
