var getECTSOPO = function(){
	var currentLocation = window.location.href;
	var urlfromid = currentLocation.replace("https://thomasmore.instructure.com/courses/","");
	var courseid = urlfromid;
	if (urlfromid.indexOf("/") > 0)
		courseid = urlfromid.substring(1,urlfromid.indexOf("/"));
	
	$('a[title^="ECTS"]').attr('href','https://student.thomasmore.be/ectsopo?id=' + courseid);
}
$(document).ready(function(){
	getECTSOPO();	
});